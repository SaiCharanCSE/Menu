#include <stdio.h>
int main()
{
    int x;
    printf ("Enter The Food Item Number:");
    scanf ("%d",&x);
    switch (x)
    {
        case 1:
              {
                printf("Food Item - Pizza\nPrice - 239");
                break;
              }
        case 2:
            {
                printf("Food Item - Burger\nPrice - 129");
                break;
            }
        case 3:
            {
                printf("Food Item - Pasta\nPrice - 179");
                break;
            }
        case 4:
            {
                printf("Food Item - French Fries\nPrice - 99");
                break;
            }
        case 5:
            {
                printf("Food Item - Sandwich\nPrice - 149");
                break;
            }
        default:
            {
                printf ("Item not available");
                break;
            }
        }
    return 0;
}
